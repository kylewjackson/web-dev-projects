{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Kyle Jackson's front-end portfolio",
  "scripts": {
    "transpile": "babel scripts -d babel",
    "scss": "sass sass:styles",
    "prefix": "postcss styles/*.css  --use autoprefixer postcss-clean -d styles/min --no-map",
	"copy-css": "mkdir -p public/styles && cp styles/min/*.css public/styles/",
    "build": "run-s transpile scss prefix copy-css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-preset-minify": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "autoprefixer": "^9.7.6",
    "core-js": "^3.6.5",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.1",
    "uniq": "^1.0.1"
  },
  "browserslist": [
    "> 0.25%",
    "last 4 versions"
  ]
